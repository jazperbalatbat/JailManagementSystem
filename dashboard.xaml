<Window x:Class="JailManagementSystem.dashboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:JailManagementSystem"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        TextElement.Foreground="{DynamicResource MaterialDesignBody}"
        TextElement.FontWeight="Regular"
        TextElement.FontSize="13"
        TextOptions.TextFormattingMode="Ideal"
        TextOptions.TextRenderingMode="Auto"
        Background="{DynamicResource MaterialDesignPaper}"
        FontFamily="{StaticResource MaterialDesignFont}"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Title="ADMIN" Height="775" Width="1370" Icon="images/jaillogo.png" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" WindowStyle="None">

    <!--main panel-->
    <StackPanel Orientation="Vertical">
        <Button Background="{x:Null}" Margin="1315,5,5,0" Padding="-20" Click="Button_Click_5">
            <materialDesign:PackIcon Kind="Close"/>
        </Button>
        <Grid Height="710" Margin="0 10 0 0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1.38*" />
                <ColumnDefinition Width="4*" />
            </Grid.ColumnDefinitions>

            <!--buttons of entities-->
            <Grid Margin="10">

                <materialDesign:Card Padding="10" Margin="0" Height="700">

                    <StackPanel Orientation="Vertical">
                        <StackPanel Orientation="Horizontal">
                            <TextBlock x:Name="main_position" Margin="0 0 70 0" Width="200" FontSize="25" FontWeight="DemiBold" HorizontalAlignment="Center"/>
                            <Button x:Name="notifications" Padding="0" ToolTip="Notifications">
                                <materialDesign:PackIcon Kind="Bell" Width="40" Height="32"/>
                            </Button>
                        </StackPanel>

                        <Image Source="images/admin.png" Margin="0 20 0 20" Width="175" Height="130"/>
                        <materialDesign:PopupBox Height="30" Grid.Column="3" Margin="180 -60 0 0">
                            <materialDesign:PopupBox.ToggleContent>
                                <materialDesign:PackIcon Kind="Settings" Width="40" Height="29" Foreground="White" />
                            </materialDesign:PopupBox.ToggleContent>
                            <StackPanel Orientation="Horizontal"  Background="Black">
                                <Button ToolTip="Edit Information" Click="Button_Click_7">
                                    <materialDesign:PackIcon Kind="Wrench" Width="25" Height="19" Foreground="White" />
                                </Button>
                                <Separator/>
                                <Button Content="SIGN OUT" FontWeight="Bold" FontSize="11" Click="Button_Click_11"/>
                            </StackPanel>
                        </materialDesign:PopupBox>
                        <StackPanel Orientation="Vertical">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="2*"/>
                                    <ColumnDefinition Width="2*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" x:Name="main_id" Margin="5,5,0,3" FontSize="25" Height="22" RenderTransformOrigin="0.5,0.5"/>
                                <TextBlock Grid.Column="1" x:Name="main_lastname" Margin="0,5,0,3" FontSize="25" Height="30"/>
                                <TextBlock Grid.Column="2" x:Name="main_firstname" Margin="0,5,0,3" FontSize="25" Height="32"/>
                            </Grid>

                        </StackPanel>

                        <Separator Margin="0 0 0 5"/>

                        <Button Content="FILE MAINTENTANCE" Margin="5" FontSize="27" FontFamily="Segoe UI Semibold" Height="70" Click="Button_Click_2"/>
                        <!--<Button x:Name="jailofficer_b" Click="Button_Click_8" Content="JAIL OFFICER" Margin="5" FontSize="22" FontFamily="Segoe UI Semibold" Height="40"/>-->

                        <Button Click="Button_Click_14" Content="TRANSACTIONS" Margin="5" FontSize="30" FontFamily="Segoe UI Semibold" Height="70"/>

                        <ScrollViewer  x:Name="transaction_list" Visibility="Collapsed" Height="200" VerticalScrollBarVisibility="Visible" CanContentScroll="True">
                            <StackPanel Orientation="Vertical">
                                <Button Content="PRISONER" Width="180" Margin="10" FontSize="18" FontFamily="Segoe UI Semibold" Height="40"/>
                                <Button x:Name="visitor_log" Click="visitor_log_Click" Content="VISITOR" Width="180" Margin="10" FontSize="18" FontFamily="Segoe UI Semibold" Height="40"/>
                                <Button Content="BAIL" Width="180" Margin="10" FontSize="18" FontFamily="Segoe UI Semibold" Height="40"/>
                                <Button x:Name="cell_monitoring" Click="cell_monitoring_Click" Content="CELL MONITORING" Width="180" Margin="10" FontSize="18" FontFamily="Segoe UI Semibold" Height="40"/>
                                <Button x:Name="hearing_schedule" Click="hearing_schedule_Click" Content="HEARING" Width="180" Margin="10" FontSize="18" FontFamily="Segoe UI Semibold" Height="40"/>
                                <Button Content="PAROLE" Width="180" Margin="10" FontSize="18" FontFamily="Segoe UI Semibold" Height="40"/>
                                <Button Content="CELL TRANSFER" Width="180" Margin="10" FontSize="18" FontFamily="Segoe UI Semibold" Height="40"/>
                                <Button Content="JAIL TRANSFER" Width="180" Margin="10" FontSize="18" FontFamily="Segoe UI Semibold" Height="40"/>
                            </StackPanel>

                        </ScrollViewer>

                        <Button Content="REPORTS" Margin="5" FontSize="30" FontFamily="Segoe UI Semibold" Height="70" Click="Button_Click_3"/>
                        <Button x:Name="util" Click="Button_Click_12" Content="UTILITIES" Margin="5 5 5 20" FontSize="30" FontFamily="Segoe UI Semibold" Height="70"/>

                    </StackPanel>
                </materialDesign:Card>
            </Grid>
            <!--buttons of entities-->

            <!--panel of entities-->
            <Grid Grid.Column="1">

                <!--file maintenance-->
                <materialDesign:Card Padding="0" Margin="0 12 10 10" x:Name="b_prisoner" Visibility="Hidden">
                    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Hidden" >
                        <StackPanel Orientation="Vertical"  >
                            <Button Width="47" Height="40" Margin="930 5 45 0" Background="{x:Null}" Click="Button_Click_4">
                                <materialDesign:PackIcon Kind="Close"/>
                            </Button>
                            <TextBlock Margin="5" Text="FILE MAINTENANCE" FontSize="45" FontWeight="DemiBold" Width="988" />
                            <Separator Margin="20"/>


                            <!--PRISONER VISITOR-->
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Text="PRISONER" FontSize="30" FontWeight="Bold" Margin="180 20 170 0"/>
                                <TextBlock Grid.Column="1" Text="VISITOR" FontSize="30" FontWeight="Bold" Margin="229,20,170,0"/>
                            </Grid>

                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Button x:Name="prisoner_b" Grid.Column="0" Height="250" Width="290" Click="Button_Click" IsEnabled="True">
                                    <Button.Background>
                                        <ImageBrush ImageSource="images/prisoner.png"/>
                                    </Button.Background>
                                </Button>
                                <Button x:Name="visitor_b" IsEnabled="True" Grid.Column="1" Height="250" Width="290" Click="Button_Click_1">
                                    <Button.Background>
                                        <ImageBrush ImageSource="images/visitor.png"/>
                                    </Button.Background>
                                </Button>
                            </Grid>
                            <!--PRISONER VISITOR-->

                            <!--CELL CASE-->
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Text="CELL" FontSize="30" FontWeight="Bold" Margin="230 20 170 0"/>
                                <TextBlock Grid.Column="1" Text="CASE" FontSize="30" FontWeight="Bold" Margin="230,20,170,0"/>
                            </Grid>

                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Button x:Name="cell_b" IsEnabled="True" Grid.Column="0 " Height="250" Width="290" Margin="103,0" VerticalAlignment="Bottom" Click="Cell_Click">
                                    <Button.Background>
                                        <ImageBrush ImageSource="images/cell.png"/>
                                    </Button.Background>
                                </Button>
                                <Button x:Name="case_b" Click="case_Click" Grid.Column="1" Height="250" Width="290">
                                    <Button.Background>
                                        <ImageBrush ImageSource="images/cases.png"/>
                                    </Button.Background>
                                </Button>
                            </Grid>
                            <!--CELL CASE-->

                            <!--ESCORT JAIL OFFUCER-->
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="1" Text="JAIL OFFICER" FontSize="30" FontWeight="Bold" Margin="210,20,170,0"/>
                                <TextBlock Grid.Column="0" Text="ESCORT" FontSize="30" FontWeight="Bold" Margin="200 20 170 0"/>
                            </Grid>

                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Button x:Name="escort_b" IsEnabled="True" Grid.Column="0" Click="escort_Click" Height="250" Width="290">
                                    <Button.Background>
                                        <ImageBrush ImageSource="images/policeescort.png"/>
                                    </Button.Background>
                                </Button>
                                <Button x:Name="jail_officer_b_fm" Grid.Column="1" Height="250" Width="290" Click="jail_officer_b_fm_Click">
                                    <Button.Background>
                                        <ImageBrush ImageSource="images/jailofficer.png"/>
                                    </Button.Background>
                                </Button>
                            </Grid>
                            <!--ESCORT JAIL OFFICER-->

                            <!--JUDGES TRIAL COURTS-->
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Text="JUDGES" FontSize="30" FontWeight="Bold" Margin="190,20,170,0"/>
                                <TextBlock Grid.Column="1" Text="TRIAL COURTS" FontSize="30" FontWeight="Bold" Margin="160 20 140 0"/>
                            </Grid>

                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Button x:Name="judges_b" Click="judges_b_Click" IsEnabled="True" Grid.Column="0" Height="250" Width="290" Margin=" 0 0 0 50">
                                    <Button.Background>
                                        <ImageBrush ImageSource="images/attorney.png"/>
                                    </Button.Background>
                                </Button>
                                <Button Click="trial_courts_b_Click" x:Name="trial_courts_b" Grid.Column="1" Height="250" Width="290" Margin=" 0 0 0 50">
                                    <Button.Background>
                                        <ImageBrush ImageSource="images/trialcourts.png"/>
                                    </Button.Background>
                                </Button>
                            </Grid>
                            <!--JUDGES TRIAL COURTS-->

                            <!--JUDGES TRIAL COURTS-->
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Text="LAWYER" FontSize="30" FontWeight="Bold" Margin="190,20,170,0"/>
                            </Grid>

                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="*" />
                                </Grid.ColumnDefinitions>
                                <Button x:Name="attorney_b" Click="attorney_b_Click" IsEnabled="True" Grid.Column="0" Height="250" Width="290" Margin=" 0 0 0 50">
                                    <Button.Background>
                                        <ImageBrush ImageSource="images/cases.png"/>
                                    </Button.Background>
                                </Button>
                            </Grid>
                            <!--JUDGES TRIAL COURTS-->

                        </StackPanel>

                    </ScrollViewer>
                </materialDesign:Card>
                <!--file maintenance-->

                <!--EDIT ADMIN INFORMATION-->
                <materialDesign:Card Padding="0" Margin="20" x:Name="warden" Visibility="Hidden">
                    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Hidden" >
                        <StackPanel Orientation="Vertical"  >
                            <Button Width="47" Height="40" Margin="930 5 5 0" Background="{x:Null}" Click="Button_Click_6">
                                <materialDesign:PackIcon Kind="Close"/>
                            </Button>
                            <TextBlock Margin="0" Text="EDIT ADMIN INFORMATION" FontSize="35" FontWeight="DemiBold" Width="916" />
                            <Separator Margin="25"/>

                            <!--new username and new password-->
                            <Grid Margin="10 0 20 10">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Text="NEW USER NAME:" FontSize="20" FontWeight="Bold" Margin="50 5 5 5"/>
                                <TextBox x:Name="username" Grid.Column="1" />
                                <TextBlock Grid.Column="2" Text="NEW PASSWORD:" FontSize="20" FontWeight="Bold" Margin="50 5 5 5"/>
                                <PasswordBox x:Name="password" Grid.Column="3" />
                            </Grid>
                            <!--new username and new password-->

                            <!--lastname and firstname-->
                            <Grid Margin="10 0 20 10">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <TextBlock Grid.Column="0" Text="LAST NAME:" FontSize="20" FontWeight="Bold" Margin="50 5 5 5"/>
                                <TextBox x:Name="a_lastname" Grid.Column="1" />
                                <TextBlock Grid.Column="2" Text="FIRST NAME:" FontSize="20" FontWeight="Bold" Margin="50 5 5 5"/>
                                <TextBox x:Name="a_firstname" Grid.Column="3" />
                            </Grid>
                            <!--lastname and firstname-->

                            <!--proceed and cancel-->
                            <Grid Margin="10 0 20 10">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Button Click="admin_proceed_Click" x:Name="admin_proceed" Grid.Column="2" Content="PROCEED" FontSize="20" FontWeight="Bold" Margin="5 5 5 5"/>
                                <Button x:Name="admin_cancel" Grid.Column="3" Content="CANCEL" />
                            </Grid>
                            <!--proceed and cancel-->


                            <StackPanel x:Name="admin_approval" Visibility="Hidden">

                                <!--old username and old password-->
                                <Grid Margin="50">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock Grid.Column="0" Text="OLD USER NAME:" FontSize="20" FontWeight="Bold" />
                                    <TextBox Grid.Column="1" Margin="0 0 50 0" />
                                    <TextBlock Grid.Column="2" Text="OLD PASSWORD:" FontSize="20" FontWeight="Bold" />
                                    <PasswordBox Grid.Column="3" />
                                </Grid>
                                <!--old username and old password-->

                                <!--proceed and cancel-->
                                <Grid Margin="10 -10 10 30">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>
                                    <Button x:Name="admin_save" Grid.Column="2" Content="SAVE" FontSize="20" FontWeight="Bold" Margin="5 5 5 5"/>
                                    <Button Grid.Column="3" Content="CANCEL" />
                                </Grid>
                                <!--proceed and cancel-->

                            </StackPanel>
                        </StackPanel>
                    </ScrollViewer>
                </materialDesign:Card>
                <!--EDIT ADMIN INFORMATION-->

                <!--JAIL OFFICER GRANT-->
                <materialDesign:Card x:Name="jailofficer" Height="500" Padding="0" Margin="20" Visibility="Hidden">

                    <StackPanel Orientation="Vertical" Height="504"  >
                        <Button Background="{x:Null}" Margin="919,5,5,20" Padding="-20" Click="Button_Click_9">
                            <materialDesign:PackIcon Kind="Close"/>
                        </Button>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Margin="10" Text="JAIL OFFICER" FontSize="35" FontWeight="DemiBold" Width="766" />
                            <Button Content="ADD NEW JAIL OFFICER" Click="Button_Click_13"/>
                        </StackPanel>
                        <Separator Margin="25"/>


                        <Grid Height="380">

                            <!--JAIL OFFICER datagrid-->
                            <DataGrid Height="350" Margin="10,0,10,56" Visibility="Visible" x:Name="datagrid_jailofficer" FontSize="15" Grid.ColumnSpan="2" MouseDoubleClick="datagrid_jailofficer_MouseDoubleClick">
                                <!--  SelectionChanged="DataGrid_SelectionChanged" MouseDoubleClick="DataGrid_MouseDoubleClick" -->
                            </DataGrid>
                            <!--JAIL OFFICER datagrid-->

                            <!--JAIL OFFICER EDIT-->
                            <StackPanel  Margin="10 -30 10 30" Visibility="Hidden" x:Name="edit_jailofficer">
                                <Button Margin="10 10 850 10" Padding="-10" Click="Button_Click_10">
                                    <materialDesign:PackIcon Kind="ArrowLeft"/>
                                </Button>

                                <StackPanel Orientation="Horizontal">
                                    <StackPanel Orientation="Vertical" Height="267" Margin="0 0 20 0">

                                        <Grid Margin="10">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column="0" Text="ID" FontSize="20" FontWeight="Bold" Margin="10 0 10 0"/>
                                            <TextBox x:Name="id" Grid.Column="1" Width="30" FontSize="20" IsEnabled="False"/>
                                        </Grid>

                                        <Grid Margin="10">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column="0" Text="POSITION" FontSize="20" FontWeight="Bold" Margin="10 0 10 0"/>
                                            <TextBox x:Name="position" Grid.Column="1" Width="200" FontSize="20"/>
                                        </Grid>

                                        <Grid Margin="10">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column="0" Text="LASTNAME" FontSize="20" FontWeight="Bold" Margin="10 0 10 0"/>
                                            <TextBox x:Name="lastname" Grid.Column="1" Text="" Width="200" FontSize="20"/>
                                        </Grid>

                                        <Grid Margin="10">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column="0" Text="FIRSTNAME" FontSize="20" FontWeight="Bold" Margin="10 0 10 0"/>
                                            <TextBox x:Name="firstname" Grid.Column="1" Text="" Width="200" FontSize="20"/>
                                        </Grid>

                                        <Grid Margin="10 10 10 15">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>

                                            <TextBlock Grid.Column="0" Text="MIDDLENAME" FontSize="20" FontWeight="Bold" Margin="10 0 10 0"/>
                                            <TextBox x:Name="middlename" Grid.Column="1" Text="" Width="200" FontSize="20"/>
                                        </Grid>

                                    </StackPanel>

                                    <StackPanel Orientation="Vertical" Height="295">

                                        <Grid Margin="0">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Margin="10" Text="GRANT ACCESS" FontSize="28" FontWeight="Bold" Foreground="#DD607585"/>
                                            <materialDesign:PackIcon Kind="AccountMultiplePlus" Grid.Column="1" Width="50" Height="50" Foreground="#DD778C9C"/>
                                            <Button Grid.Column="2" x:Name="grant_access" Content="GRANT ACCESS" Width="200" Click="grant_access_Click" Margin="10"/>
                                        </Grid>


                                        <Grid Margin="10 10 10 15">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>

                                            <materialDesign:Card Grid.Column="0" Margin="5" Width="280">
                                                <StackPanel Orientation="Vertical">
                                                    <TextBlock Text="PRISONER INFORMATION"  Margin="20" FontSize="20" FontWeight="Bold" />
                                                    <CheckBox x:Name="access_prisoner" Content="PRISONER" Margin="5" FontWeight="Bold"/>
                                                    <CheckBox x:Name="access_visitor" Content="VISITOR" Margin="5" FontWeight="Bold"/>
                                                    <CheckBox x:Name="access_cell" Content="CELL" Margin="5" FontWeight="Bold"/>
                                                    <CheckBox x:Name="access_case" Content="CASE" Margin="5" FontWeight="Bold"/>
                                                    <CheckBox x:Name="access_escort" Content="ESCORT" Margin="5 5 5 15" FontWeight="Bold"/>
                                                </StackPanel>
                                            </materialDesign:Card>

                                            <materialDesign:Card Grid.Column="1" Margin="5">
                                                <StackPanel Orientation="Vertical">
                                                    <TextBlock Text="REPORTS"  Margin="2" FontSize="20" FontWeight="Bold"/>
                                                    <CheckBox x:Name="access_prisonerreport" Content="PRISONER REPORT" Margin="5" FontWeight="Bold"/>
                                                    <CheckBox x:Name="access_visitorreport" Content="VISITOR REPORT" Margin="5" FontWeight="Bold"/>
                                                    <CheckBox x:Name="access_cellreport" Content="CELL REPORT" Margin="5" FontWeight="Bold"/>
                                                    <CheckBox x:Name="access_casereport" Content="CASE REPORT" Margin="5" FontWeight="Bold"/>
                                                    <CheckBox x:Name="access_bail_report" Content="BAIL REPORT" Margin="5" FontWeight="Bold"/>
                                                    <CheckBox x:Name="access_hearing_report" Content="HEARING REPORT" Margin="5" FontWeight="Bold"/>
                                                    <CheckBox x:Name="access_jailofficerreport" Content="JAIL OFFICER REPORT" Margin="5 5 5 15" FontWeight="Bold"/>
                                                </StackPanel>
                                            </materialDesign:Card>
                                        </Grid>
                                    </StackPanel>

                                </StackPanel>
                            </StackPanel>


                            <!--JAIL OFFICER EDIT-->

                            <!--JAIL OFFICER datagrid-->


                        </Grid>




                    </StackPanel>
                </materialDesign:Card>
                <!--JAIL OFFICER GRANT-->

                <!--reports-->

                <materialDesign:Card Padding="0" Margin="20" x:Name="reports" Visibility="Hidden">
                    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Hidden" Height="450" >
                        <StackPanel Orientation="Vertical"  >
                            <TextBlock Margin="0,15,0 0" Text="REPORTS" FontSize="35" FontWeight="Bold" Width="916" />
                            <Separator Margin="25"/>
                            <CheckBox Margin="15,0,0,0" FontSize="30" FontWeight="DemiBold" x:Name="prisoner" Checked="prisoner_CheckedChanged">PRISONER REPORTS</CheckBox>
                            <CheckBox Margin="15,0,0,0" FontSize="30" FontWeight="DemiBold" x:Name="visitor" Checked="visitor_CheckedChanged" >VISITOR REPORTS</CheckBox>
                            <CheckBox Margin="15,0,0,0" FontSize="30" FontWeight="DemiBold" x:Name="cell" Checked="cell_CheckedChanged">CELL REPORTS</CheckBox>
                            <CheckBox Margin="15,0,0,0" FontSize="30" FontWeight="DemiBold" x:Name="case" Checked="cases_CheckedChanged" >CASES REPORTS</CheckBox>
                            <CheckBox Margin="15,0,0,0" FontSize="30" FontWeight="DemiBold" x:Name="bail" Checked="bail_CheckedChanged" >BAIL REPORTS</CheckBox>
                            <CheckBox Margin="15,0,0,0" FontSize="30" FontWeight="DemiBold" x:Name="hearing" Checked="hearing_CheckedChanged" >HEARING SCHEDULE REPORTS</CheckBox>
                            <CheckBox Margin="15,0,0,0" FontSize="30" FontWeight="DemiBold" x:Name="officer" Checked="officer_CheckedChanged" >JAIL OFFICER REPORTS</CheckBox>
                        </StackPanel>
                    </ScrollViewer>
                </materialDesign:Card>
                <!--reports-->

                <!--utilities-->

                <materialDesign:Card Padding="0" Margin="20" x:Name="utilities" Visibility="Hidden">
                    <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Hidden" Height="270" >
                        <StackPanel Orientation="Vertical"  >
                            <TextBlock Margin="0,15,0 0" Text="UTILITIES" FontSize="35" FontWeight="Bold" Width="916" />
                            <Separator Margin="25"/>
                            <CheckBox Margin="15,0,0,0" FontSize="30" FontWeight="DemiBold" x:Name="audittrail" Checked="audittrail_CheckedChanged">AUDIT TRAIL</CheckBox>
                            <CheckBox Visibility="Collapsed" Margin="15,0,0,0" FontSize="30" FontWeight="DemiBold" x:Name="Backup" Checked="Backup_CheckedChanged">BACKUP AND RESTORE</CheckBox>
                            <CheckBox Margin="15,0,0,0" FontSize="30" FontWeight="DemiBold" x:Name="archive" Checked="archive_CheckedChanged">ARCHIVE</CheckBox>



                            <!--<CheckBox Margin="15,0,0,0" FontSize="30" FontWeight="DemiBold" x:Name="archive" Checked="archive_CheckedChanged" >ARCHIVE</CheckBox>-->
                        </StackPanel>
                    </ScrollViewer>
